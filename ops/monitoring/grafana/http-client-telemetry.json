{
  "title": "Medical KG - HTTP Client Telemetry",
  "uid": "medkg-http-telemetry",
  "tags": ["medical-kg", "telemetry"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "reqps"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "id": 1,
      "title": "Request Rate by Host",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (host)",
          "legendFormat": "{{host}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "percentunit"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "id": 2,
      "title": "HTTP Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status!~\"2..\"}[5m])) / sum(rate(http_requests_total[5m]))",
          "legendFormat": "error rate",
          "refId": "A"
        }
      ],
      "thresholds": {
        "mode": "absolute",
        "steps": [
          {"color": "green"},
          {"color": "orange", "value": 0.02},
          {"color": "red", "value": 0.05}
        ]
      }
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "s"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "id": 3,
      "title": "Latency Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, host))",
          "legendFormat": "P50 {{host}}",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, host))",
          "legendFormat": "P95 {{host}}",
          "refId": "B"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "bytes"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "id": 4,
      "title": "Response Size Percentiles",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum(rate(http_response_size_bytes_bucket[5m])) by (le))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_response_size_bytes_bucket[5m])) by (le))",
          "legendFormat": "P95",
          "refId": "B"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "reqps"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "id": 5,
      "title": "Retry Rate by Reason",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_retries_total[5m])) by (reason)",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "s"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "id": 6,
      "title": "Backoff Duration P95",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_backoff_duration_seconds_bucket[5m])) by (le, host))",
          "legendFormat": "P95 {{host}}",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "none"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "id": 7,
      "title": "Limiter Queue Depth",
      "type": "timeseries",
      "targets": [
        {
          "expr": "max_over_time(http_limiter_queue_depth[5m])",
          "legendFormat": "queue depth",
          "refId": "A"
        }
      ]
    },
    {
      "datasource": {"type": "prometheus", "uid": "PROMETHEUS_DS"},
      "fieldConfig": {
        "defaults": {"unit": "percentunit"},
        "overrides": []
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "id": 8,
      "title": "Queue Saturation",
      "type": "timeseries",
      "targets": [
        {
          "expr": "avg_over_time(http_limiter_queue_saturation[5m])",
          "legendFormat": "saturation",
          "refId": "A"
        }
      ]
    }
  ]
}
